<div class="row" >
    <div class="col">
        
    </div>
    <div class="col-8">
        <div class="row">
            <div class="col s12 m6">
              <div class="card brown lighten-5">
                <div class="card-content">
                    <!-- form starts here -->
                    <form class="col s12">
                        <div class="row white-text">
                          <div class="input-field col s12">
                            <input  value="<%= record.environmentName %>" id="disabled" type="text" class="validate">
                            <label for="env_name">Environment Name</label>
                          </div>
                        </div>
                        <div class="row">
                            <div class="input-field col s6">
                                <input value="<%= record.environmentCreator %>" id="creator" type="text" class="validate">
                                <label for="creator">Environment Creator</label>
                              </div>
                        </div>
                        <!-- Environment Options stats here -->
                        <div class="row">
                            <div class="col s12 m6">
                              <div class="card brown lighten-4">
                                <div class="card-content white-text">
                                  <span class="card-title">Environment Options</span>
                                  <div class="row">
                                    <div class="input-field col s6">
                                        <input value="<%= record.environmentOptions.panorama %>" id="panorama" type="text" class="validate">
                                        <label for="panorama">Panorama</label>
                                      </div>
                                </div>
                                <div class="row">
                                    <div class="input-field col s6">
                                        <input value="<%= record.environmentOptions.preset %>" id="preset" type="text" class="validate">
                                        <label for="preset">preset</label>
                                      </div>
                                </div>
                                <div class="row">
                                    <div class="input-field col s6">
                                        <input value="<%= record.environmentOptions.preset %>" id="video" type="text" class="validate">
                                        <label for="video">video</label>
                                      </div>
                                </div>
                                <!-- Floor Assets starts here -->
                                <div class="row">
                                    <div class="col s12 m6">
                                      <div class="card brown lighten-3">
                                        <div class="card-content white-text">
                                          <span class="card-title">Sky Assets</span>
                                          <div class="row">
                                            <div class="input-field col s6">
                                                <input value="<%= record.environmentOptions.skyAsset.color %>" id="skyColor" type="text" class="validate">
                                                <label for="skyColor">sky color</label>
                                              </div>
                                        </div>
                                        <div class="row">
                                            <div class="input-field col s6">
                                                <input value="<%= record.environmentOptions.skyAsset.url %>" id="skyUrl" type="text" class="validate">
                                                <label for="skyUrl">sky url</label>
                                              </div>
                                        </div>
                                        </div>
                                      </div>
                                    </div>
                                  </div>
                                  <!-- Floor Assets ends here -->
                                  <!-- Sky Assets starts here  -->
                                  <div class="row">
                                    <div class="col s12 m6">
                                      <div class="card brown lighten-3">
                                        <div class="card-content white-text">
                                          <span class="card-title">Floor Assets</span>
                                          <div class="row">
                                            <div class="input-field col s6">
                                                <input value="<%= record.environmentOptions.floorAsset.color %>" id="floorColor" type="text" class="validate">
                                                <label for="floorColor">floor color</label>
                                              </div>
                                        </div>
                                        <div class="row">
                                            <div class="input-field col s6">
                                                <input value="<%= record.environmentOptions.floorAsset.url %>" id="floorUrl" type="text" class="validate">
                                                <label for="floorUrl">floor url</label>
                                              </div>
                                        </div>
                                        </div>
                                      </div>
                                    </div>
                                  </div>
                                <!-- Sky Assets ends here  -->
                                
                                </div>
                              </div>
                            </div>
                          </div>
                          <!-- Environment Options ends here -->
                          
                          <% record.vrObject.forEach(object=>{ %>
                          <div class="row">
                            <div class="col s12 m6">
                              <div class="card blue-grey darken-1">
                                <div class="card-content white-text">
                                  <span class="card-title"><%= object.name %></span>
                                  <div class="row">
                                    <div class="input-field col s6">
                                        <input value="<%= object.name %>" id="obj_name<%= object._id %>" type="text" class="validate">
                                        <label for="<%= object.name %>">name</label>
                                      </div>
                                </div>
                                <div class="row">
                                    <div class="input-field col s6">
                                        <input value="<%= object.position %>" id="obj_position<%= object._id %>" type="text" class="validate">
                                        <label for="obj_position">position</label>
                                      </div>
                                </div>
                                <div class="row">
                                    <div class="input-field col s6">
                                        <input value="<%= object.scale %>" id="obj_scale<%= object._id %>" type="text" class="validate">
                                        <label for="obj_scale">scale</label>
                                      </div>
                                </div>
                                <div class="row">
                                    <div class="input-field col s6">
                                        <input value="<%= object.rotation %>" id="obj_rotation<%= object._id %>" type="text" class="validate">
                                        <label for="obj_rotation">rotation</label>
                                      </div>
                                </div>
                                <div class="row">
                                    <div class="input-field col s6">
                                        <input value="<%= object.url %>" id="obj_url<%= object._id %>" type="text" class="validate">
                                        <label for="obj_url">url</label>
                                      </div>
                                </div>
                                </div>
                                <div class="card-action">
                                    <a id="<%= object._id %>"   class="waves-effect waves-light btn update">update</a>
                                    <a id="<%= object._id %>"   class="waves-effect waves-light btn delete">delete</a>
                                    
                                </div>
                              </div>
                            </div>
                          </div>
                          
                          <% } )%> 
                          <div class="card-action">
                            <a class="waves-effect waves-light btn modal-trigger" href="#modal1">Add Object</a>
                          </div>
                      </form>
                      
                      <!-- form ends here -->
                </div>
                <div class="card-action">
                  <a href="#">This is a link</a>
                </div>
              </div>
            </div>
          </div>
    </div>
    <div class="col">
        
    </div>
    
    
  </div>

  <!-- Modal Structure -->
  <div id="modal1" class="modal">
    <div class="modal-content">
      <h4>Add new 3D object</h4>
    </div>
    <div class="row">
        <div class="row">
            <div class="col s12 m6">
              <div class="card blue-grey darken-1">
                <div class="card-content white-text">
                  <span class="card-title"></span>
                  <div class="row">
                    <div class="input-field col s6">
                        <input  id="obj_name" type="text" class="validate">
                        <label for="obj_name">name</label>
                      </div>
                </div>
                <div class="row">
                    <div class="input-field col s6">
                        <input  id="obj_position" type="text" class="validate">
                        <label for="obj_position">position</label>
                      </div>
                </div>
                <div class="row">
                    <div class="input-field col s6">
                        <input  id="obj_scale" type="text" class="validate">
                        <label for="obj_scale">scale</label>
                      </div>
                </div>
                <div class="row">
                    <div class="input-field col s6">
                        <input  id="obj_rotation" type="text" class="validate">
                        <label for="obj_rotation">rotation</label>
                      </div>
                </div>
                <div class="row">
                    <div class="input-field col s6">
                        <input  id="obj_url" type="text" class="validate">
                        <label for="obj_url">url</label>
                      </div>
                </div>
                </div>
              </div>
            </div>
          </div>
    </div>
    <div class="modal-footer">
        
       <div class="card-action">
        <a id="<%= record._id %>"   class="waves-effect waves-light btn add-obj">upload</a>
       </div>
        
    </div>
  </div>